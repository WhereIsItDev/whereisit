{
  "manifest_version": 2,

  "name": "Where is it 1.0",
  "description": "Searches through Github repository to find method definition",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "find.html"
  },
  "content_scripts":[
    {
      "matches":["*://github.com/*"],
      "js":["injected.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "*://github.com/*",
    "https://ajax.googleapis.com/"
  ]
}