{
  "manifest_version": 2,

  "name": "Where is it 1.0",
  "description": "Searches through Github repository to find method definition",
  "version": "1.0",
  "background":
    {
        "scripts": ["jquery-2.1.4.js", "find.js"]
    },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "find.html"
  },
  "content_scripts":[
    {
      "matches":["*://github.com/*"],
      "js":["injected.js",
      "jquery-2.1.4.js",
      "find.js"],
            "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab",
    "*://github.com/*",
    "https://ajax.googleapis.com/",
    "http://localhost/",
    "http://45.33.82.241/"
  ]
}